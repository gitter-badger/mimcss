{
	"compileOnSave": true,
	"compilerOptions":
	{
		"declaration": true,
		"sourceMap": true,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"module": "commonjs",
		"target": "ES2017",
		"lib": [ "ES2017", "dom" ],
		"experimentalDecorators": true,
		"jsx": "react",
		"preserveConstEnums": false
	},
}